{
  "tsserver.enable": true,
  "tsserver.enableJavascript": true,
  "typescript.suggest.enabled": true,
  "typescript.suggestionActions.enabled": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.previewAutoClose": true,
  "vetur.completion.useScaffoldSnippets": true,
  "codeLens.enable": true,
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": true,
  "javascript.implementationsCodeLens.enable": true,
  "typescript.suggest.paths": true,
  "tsserver.formatOnType": true,
  "tslint.jsEnable": true,
  "tslint.autoFixOnSave": true,
  "coc.preferences.formatOnType": true,
  "javascript.referencesCodeLens.enable": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "emmet.includeLanguages": {"vue-html": "html"},
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
}
